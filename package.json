{
    "name": "celerity",
    "version": "65EEA59C",
    "description": "A music Discord bot focused on performance, without sacrificing functionality, features and quality.",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "prettier": "prettier --write .",
        "cloc": "cloc --quiet --json --exclude-dir=node_modules,dist . > lines.json",
        "version": "new_version=\"$(echo \"obase=16; $(date +%s)\" | bc)\" && jq --arg new_version \"$new_version\" '.version = $new_version' package.json > package_temp.json && mv package_temp.json package.json && npm install --silent && npm run cloc > /dev/null && prettier --write . > /dev/null && echo chore\\(ver\\): bump version to $new_version"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thaddeuskkr/Celerity.git"
    },
    "keywords": [
        "discordjs",
        "discord-bot"
    ],
    "author": "thaddeus kuah (thaddeuskkr)",
    "license": "AGPL-3.0-or-later",
    "bugs": {
        "url": "https://github.com/thaddeuskkr/Celerity/issues"
    },
    "homepage": "https://github.com/thaddeuskkr/Celerity#readme",
    "dependencies": {
        "@discordjs/collection": "^2.0.0",
        "@keyv/mongo": "^2.2.8",
        "@sapphire/discord.js-utilities": "^7.1.6",
        "@top-gg/sdk": "^3.1.6",
        "axios": "^1.6.7",
        "bufferutil": "^4.0.8",
        "chalk": "^5.3.0",
        "common-tags": "^1.8.2",
        "discord.js": "^14.14.1",
        "dotenv": "^16.4.5",
        "erlpack": "github:discord/erlpack",
        "fast-deep-equal": "^3.1.3",
        "keyv": "^4.5.4",
        "lodash": "^4.17.21",
        "luxon": "^3.4.4",
        "pino": "^8.19.0",
        "pino-pretty": "^10.3.1",
        "pretty-ms": "^9.0.0",
        "shoukaku": "^4.0.1",
        "utf-8-validate": "^6.0.3",
        "zlib-sync": "^0.1.9"
    },
    "devDependencies": {
        "@types/common-tags": "^1.8.4",
        "@types/express": "^4.17.21",
        "@types/lodash": "^4.14.202",
        "@types/luxon": "^3.4.2",
        "@types/node": "^20.11.25",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "@typescript-eslint/parser": "^7.1.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-deprecation": "^2.0.0",
        "node-gyp": "^10.0.1",
        "prettier": "^3.2.5",
        "typescript": "^5.4.2"
    },
    "engines": {
        "node": ">=18"
    }
}
